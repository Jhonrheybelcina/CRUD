{"ast":null,"code":"import axios from 'axios';\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: 'Shoe',\n  data() {\n    return {\n      isModalOpen: false,\n      brand: '',\n      model: '',\n      size: null,\n      color: '',\n      price: null,\n      loading: false,\n      shoes: []\n    };\n  },\n  created() {\n    // Fetch shoe data from the Go backend when the component is created\n    axios.get('http://localhost:8080/showAll', {}).then(response => {\n      this.shoes = response.data;\n      this.loading = false;\n    }).catch(error => {\n      console.error('Error fetching shoe data:', error);\n      this.error = 'Error fetching shoe data. Please try again later.';\n      this.loading = false;\n    });\n  },\n  methods: {\n    openModal() {\n      this.isModalOpen = true;\n    },\n    closeModal() {\n      this.isModalOpen = false;\n    },\n    createShoe() {\n      this.loading = true;\n      const shoeData = {\n        brand: this.brand,\n        model: this.model,\n        size: this.size,\n        color: this.color,\n        price: this.price\n      };\n      axios.post('http://localhost:8080/create', shoeData).then(response => {\n        console.log('Success:', response.data);\n        window.alert(\"Shoe added successfully\");\n        window.location.reload();\n      }).catch(error => {\n        console.error('Error:', error);\n        window.alert(\"Error adding shoe. Please try again.\");\n      }).finally(() => {\n        this.loading = false;\n        this.closeModal();\n      });\n    },\n    confirmDelete(shoeId) {\n      if (confirm(\"Are you sure you want to delete this shoe?\")) {\n        // User clicked OK, proceed with deletion\n        this.deleteShoe(shoeId);\n      } else {\n        // User clicked Cancel, do nothing\n      }\n    },\n    deleteShoe(shoeId) {\n      this.loading = true;\n      axios.delete(`http://localhost:8080/delete/${shoeId}`).then(response => {\n        console.log('Success:', response.data);\n        window.alert(\"Shoe deleted successfully\");\n        window.location.reload();\n      }).catch(error => {\n        console.error('Error:', error);\n        window.alert(\"Error deleting shoe. Please try again.\");\n      }).finally(() => {\n        this.loading = false;\n        window.location.reload(); // Reload the page inside the finally block\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","isModalOpen","brand","model","size","color","price","loading","shoes","created","get","then","response","catch","error","console","methods","openModal","closeModal","createShoe","shoeData","post","log","window","alert","location","reload","finally","confirmDelete","shoeId","confirm","deleteShoe","delete"],"sources":["D:\\Ganknow Projects\\Vue\\frontend\\src\\components\\pages\\Shoe.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <Modal v-if=\"isModalOpen\" @close=\"closeModal\">\r\n      <!-- Content of the modal goes here -->\r\n      <div class=\"card\">\r\n        <form @submit.prevent=\"createShoe\">\r\n          <div class=\"form-group\">\r\n            <label for=\"brand\">Brand</label>\r\n            <input id=\"brand\" v-model=\"brand\" required>\r\n            <div v-if=\"!brand\" class=\"req\">Brand is required!</div>\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"model\">Model</label>\r\n            <input id=\"model\" v-model=\"model\" required>\r\n            <div v-if=\"!model\" class=\"req\">Model is required!</div>\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"size\">Size</label>\r\n            <input id=\"size\" v-model=\"size\" type=\"number\" required>\r\n            <div v-if=\"!size\" class=\"req\">Size is required!</div>\r\n          </div>\r\n\r\n          <div class=\"form-group\">\r\n            <label for=\"color\">Color</label>\r\n            <input id=\"color\" v-model=\"color\" req