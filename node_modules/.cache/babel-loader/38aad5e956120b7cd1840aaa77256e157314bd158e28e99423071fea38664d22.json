{"ast":null,"code":"import axios from 'axios';\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: 'Shoe',\n  data() {\n    return {\n      shoes: [],\n      loading: true,\n      error: null,\n      modalVisible: false,\n      brand: '',\n      image: null // Variable to store the selected image file\n    };\n  },\n\n  mounted() {\n    const token = localStorage.getItem('token'); // Assuming the server provides a token upon successful login\n\n    // Fetch shoe data from the Go backend when the component is mounted\n    axios.get('http://localhost:8080/protected/showAll', {\n      headers: {\n        Authorization: `Bearer ${token}`\n      }\n    }).then(response => {\n      this.shoes = response.data;\n      this.loading = false;\n    }).catch(error => {\n      console.error('Error fetching shoe data:', error);\n      this.error = 'Error fetching shoe data. Please try again later.';\n      this.loading = false;\n    });\n  },\n  methods: {\n    openModal() {\n      this.modalVisible = true;\n    },\n    createShoe() {\n      // Create a FormData object to send the form data\n      const formData = new FormData();\n      formData.append('brand', this.brand);\n      // Append other form fields (model, size, color, price) to the FormData object\n      formData.append('image', this.image);\n      const token = localStorage.getItem('token');\n\n      // Make a POST request to the backend endpoint\n      axios.post('http://localhost:8080/protected/create', formData, {\n        headers: {\n          Authorization: `Bearer ${token}`,\n          'Content-Type': 'multipart/form-data'\n        }\n      }).then(response => {\n        console.log(response.data);\n        // Handle the response as needed\n      }).catch(error => {\n        console.error('Error:', error);\n        // Handle errors\n      }).finally(() => {\n        // Close the modal regardless of success or failure\n        this.modalVisible = false;\n      });\n    }\n  }\n