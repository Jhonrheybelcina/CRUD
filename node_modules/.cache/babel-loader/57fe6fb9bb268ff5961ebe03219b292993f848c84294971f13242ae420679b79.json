{"ast":null,"code":"import axios from 'axios';\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: 'Shoe',\n  data() {\n    return {\n      isModalOpen: false,\n      brand: '',\n      model: '',\n      size: null,\n      color: '',\n      price: null,\n      loading: false\n    };\n  },\n  methods: {\n    openModal() {\n      this.isModalOpen = true;\n    },\n    closeModal() {\n      this.isModalOpen = false;\n    },\n    createShoe() {\n      this.loading = true;\n      const shoeData = {\n        brand: this.brand,\n        model: this.model,\n        size: this.size,\n        color: this.color,\n        price: this.price\n      };\n      axios.post('http://localhost:8080/create', shoeData).then(response => {\n        console.log('Success:', response.data);\n        // Handle the response as needed\n        // For example, update your UI or show a success message\n        window.location.alert(\"Shoe added successfully\");\n      }).catch(error => {\n        console.error('Error:', error);\n        // Handle errors\n        // For example, show an error message to the user\n      }).finally(() => {\n        this.loading = false;\n        this.closeModal();\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","isModalOpen","brand","model","size","color","price","loading","methods","openModal","closeModal","createShoe","shoeData","post","then","response","console","log","window","location","alert","catch","error","finally"],"sources":["D:\\Ganknow Projects\\Vue\\frontend\\src\\components\\pages\\Shoe.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <button @click=\"openModal\">Open Modal</button>\r\n    <Modal v-if=\"isModalOpen\" @close=\"closeModal\">\r\n      <!-- Content of the modal goes here -->\r\n      <div class=\"card\">\r\n        <form @submit.prevent=\"createShoe\">\r\n          <div class=\"form-group\">\r\n            <label for=\"brand\">Brand</label>\r\n            <inpu