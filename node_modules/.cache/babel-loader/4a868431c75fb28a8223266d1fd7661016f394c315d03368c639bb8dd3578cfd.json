{"ast":null,"code":"import axios from 'axios';\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: 'Shoe',\n  data() {\n    return {\n      modalVisible: false,\n      brand: '',\n      image: null // Variable to store the selected image file\n    };\n  },\n\n  methods: {\n    openModal() {\n      this.modalVisible = true;\n    },\n    closeModal() {\n      this.modalVisible = false;\n    },\n    createShoe() {\n      // Create a FormData object to send the form data\n      const formData = new FormData();\n      formData.append('brand', this.brand);\n      // Append other form fields (model, size, color, price) to the FormData object\n      // Example:\n      formData.append('model', 'YourModelValue');\n      formData.append('size', 'YourSizeValue');\n      formData.append('color', 'YourColorValue');\n      formData.append('price', 'YourPriceValue');\n      formData.append('image', this.image);\n\n      // Make a POST request to the backend endpoint\n      axios.post('http://localhost:8080/create', formData).then(response => {\n        console.log(response.data);\n        // Handle the response as needed\n      }).catch(error => {\n        console.error('Error:', error);\n        // Handle errors\n      }).finally(() => {\n        // Close the modal regardless of success or failure\n        this.closeModal();\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","modalVisible","brand","image","methods","openModal","closeModal","createShoe","formData","FormData","append","post","then","response","console","log","catch","error","finally"],"sources":["D:\\Ganknow Projects\\Vue\\frontend\\src\\components\\pages\\Shoe.vue"],"sourcesContent":["<!-- eslint-disable vue/multi-word-component-names -->\r\n<template>\r\n  <div>\r\n    <!-- Button to trigger the modal -->\r\n    <button @click=\"openModal\">Create Shoe</button>\r\n\r\n    <!-- Modal -->\r\n    <transition name=\"modal-fade\">\r\n      <div v-if=\"modalVisible\" class=\"modal-overlay\" @click=\"closeModal\">\r\n        <div class=\"modal-container\" @click.stop>\r\n          <div class=\"modal-header\">\r\n            <h3>Create Shoe</h3>\r\n            <span class=\"close-btn\" @click=\"closeModal\">&times;</span>\r\n          </div>\r\n          <div class=\"modal-body\">\r\n            <!-- Form inside the modal -->\r\n            <form @submit.prevent=\"createShoe\">\r\n              <label for=\"brand\">Brand:</label>\r\n              <input type=\"text\" id=\"brand\" v-model=\"brand\" required>\r\n\r\n              <!-- Add other form fields (model, size, color, price) -->\r\n\r\n              <label for=\"image\">Image:</label>\r\n              <input type=\"file\" id=\"image\" accept=\"image/*\">\r\n\r\n              <button type=\"submit\">Submit</button>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  // eslint-disable-next-line vue/multi-word-component-names\r\n  name: 'Shoe',\r\n  data() {\r\n    return {\r\n      modalVisible: false,\r\n      brand: '',\r\n      image: null, // Variable to store the selected image file\r\n    };\r\n  },\r\n  methods: {\r\n    openModal() {\r\n      this.modalVisible = true;\r\n    },\r\n    closeModal() {\r\n      this.modalVisible = false;\r\n    },\r\n    createShoe() {\r\n      // Create a FormData object to send the form data\r\n      const formData = new FormData();\r\n      formData.append('brand', this.brand);\r\n      // Append other form fields (model, size, color, price) to the FormData object\r\n      // Example:\r\n      formData.append('model', 'YourModelValue');\r\n      formData.append('size', 'YourSizeValue');\r\n      formData.append('color', 'YourColorValue');\r\n      formData.append('price', 'YourPriceValue');\r\n      formData.append('image', this.image);\r\n\r\n      // Make a POST request to the backend endpoint\r\n      axios.post('http://localhost:8080/create', formData)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          // Handle the response as needed\r\n        })\r\n        .catch(error => {\r\n          console.error('Error:', error);\r\n          // Handle errors\r\n        })\r\n        .finally(() => {\r\n          // Close the modal regardless of success or failure\r\n          this.closeModal();\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n<style>\r\n.modal-overlay {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.modal-container {\r\n  background: #fff;\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);\r\n}\r\n\r\n.modal-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.close-btn {\r\n  cursor: pointer;\r\n}\r\n\r\n.modal-body {\r\n  margin-top: 20px;\r\n}\r\n\r\n/* Modal fade animation */\r\n.modal-fade-enter-active,\r\n.modal-fade-leave-active {\r\n  transition: opacity 0.5s;\r\n}\r\n.modal-fade-enter, .modal-fade-leave-to /* .modal-fade-leave-active in <2.1.8 */ {\r\n  opacity: 0;\r\n}\r\n</style>\r\n"],"mappings":"AAmCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACb;EACAC,IAAI,EAAE,MAAM;EACZC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,KAAK;MACnBC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,IAAI,CAAE;IACf,CAAC;EACH,CAAC;;EACDC,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACV,IAAI,CAACJ,YAAW,GAAI,IAAI;IAC1B,CAAC;IACDK,UAAUA,CAAA,EAAG;MACX,IAAI,CAACL,YAAW,GAAI,KAAK;IAC3B,CAAC;IACDM,UAAUA,CAAA,EAAG;MACX;MACA,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/BD,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAACR,KAAK,CAAC;MACpC;MACA;MACAM,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,gBAAgB,CAAC;MAC1CF,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,eAAe,CAAC;MACxCF,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,gBAAgB,CAAC;MAC1CF,QAAQ,CAA