oogle_map js **/\r\nfunction myMap() {\r\n    var mapProp = {\r\n        center: new google.maps.LatLng(40.712775, -74.005973),\r\n        zoom: 18,\r\n    };\r\n    var map = new google.maps.Map(document.getElementById(\"googleMap\"), mapProp);\r\n}\r\n\r\n// client section owl carousel\r\n$(\".client_owl-carousel\").owlCarousel({\r\n    loop: true,\r\n    margin: 0,\r\n    dots: false,\r\n    nav: true,\r\n    navText: [],\r\n    autoplay: true,\r\n    autoplayHoverPause: true,\r\n    navText: [\r\n        '<i class=\"fa fa-angle-left\" aria-hidden=\"true\"></i>',\r\n        '<i class=\"fa fa-angle-right\" aria-hidden=\"true\"></i>'\r\n    ],\r\n    responsive: {\r\n        0: {\r\n            items: 1\r\n        },\r\n        768: {\r\n            items: 2\r\n        },\r\n        1000: {\r\n            items: 2\r\n        }\r\n    }\r\n});\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"],"mappings":"AAKA,eAAe;EACb;EACAA,IAAI,EAAE,MAAM;EACZC,OAAOA,CAAA,EAAG;IACR,MAAMC,UAAS,GAAIC,YAAY,CAACC,OAAO,CAAC,YAAY,MAAM,MAAM;IAEhE,IAAI,CAACF,UAAU,EAAE;MACf;MACAG,KAAK,CAAC,+BAA+B,CAAC;MACtCC,MAAM,CAACC,QAAQ,CAACC,IAAI,GAAC,SAAQ,EAAE;IACjC;EACF;AACF,CAAC;;AAED;AACA,SAASC,OAAOA,CAAA,EAAG;EACf,IAAIC,WAAU,GAAI,IAAIC,IAAI,CAAC,CAAC;EAC5B,IAAIC,WAAU,GAAIF,WAAW,CAACG,WAAW,CAAC,CAAC;EAC3CC,QAAQ,CAACC,aAAa,CAAC,cAAc,CAAC,CAACC,SAAQ,GAAIJ,WAAW;AAClE;AAEAH,OAAO,CAAC,CAAC;;AAGT;AACAQ,CAAC,CAACX,MAAM,CAAC,CAACY,EAAE,CAAC,MAAM,EAAE,YAAY;EAC7BD,CAAC,CAAC,kBAAkB,CAAC,CAACE,KAAK,CAAC,YAAY;IACpCF,CAAC,CAAC,kBAAkB,CAAC,CAACG,WAAW,CAAC,QAAQ,CAAC;IAC3CH,CAAC,CAAC,IAAI,CAAC,CAACI,QAAQ,CAAC,QAAQ,CAAC;IAE1B,IAAIC,IAAG,GAAIL,CAAC,CAAC,IAAI,CAAC,CAACM,IAAI,CAAC,aAAa,CAAC;IACtCC,KAAK,CAACC,OAAO,CAAC;MACVC,MAAM,EAAEJ;IACZ,CAAC;EACL,CAAC,CAAC;EAEF,IAAIE,KAAI,GAAIP,CAAC,CAAC,OAAO,CAAC,CAACQ,OAAO,CAAC;IAC3BE,YAAY,EAAE,MAAM;IACpBC,eAAe,EAAE,KAAK;IACtBC,OAAO,EAAE;MACLC,WAAW,EAAE;IACjB;EACJ,CAAC;AACL,CAAC,CAAC;;AAEF;AACAb,CAAC,CAACH,QAAQ,CAAC,CAACiB,KAAK,CAAC,YAAW;EACzBd,CAAC,CAAC,QAAQ,CAAC,CAACe,UAAU,CAAC,CAAC