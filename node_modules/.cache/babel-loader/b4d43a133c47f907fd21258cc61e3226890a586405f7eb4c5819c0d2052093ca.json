valid email first.';\r\n        return;\r\n      }\r\n\r\n      axios\r\n        .post('http://localhost:8080/forgot-password', { email: this.email, password: this.password })\r\n        // eslint-disable-next-line no-unused-vars\r\n        .then(response => {\r\n          this.passwordResetSuccessful = true;\r\n          // Clear the email and password fields\r\n          this.email = '';\r\n          this.password = '';\r\n          this.emailEntered = false; // Reset emailEntered status\r\n        })\r\n        .catch(error => {\r\n          console.error('Error resetting password:', error);\r\n          this.error = 'An error occurred while resetting the password';\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n  \r\n<style scoped>\r\n\r\n\r\n\r\n\r\n\r\n.success-message {\r\n  color: #4caf50;\r\n}\r\n\r\n.error-message {\r\n  color: #d9534f;\r\n}\r\n.forgot-form {\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    height: 100vh;\r\n    margin: 0;\r\n}\r\n\r\n.form {\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap: 10px;\r\n    background-color: #ffffff;\r\n    padding: 30px;\r\n    width: 450px;\r\n    border-radius: 20px;\r\n    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;\r\n    box-shadow: 0px 0px 10px 0px rgba(0, 0, 0, 0.1); \r\n}\r\n\r\n::placeholder {\r\n    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;\r\n}\r\n\r\n.form button {\r\n    align-self: flex-end;\r\n}\r\n\r\n.flex-column>label {\r\n    color: #151717;\r\n    font-weight: 600;\r\n}\r\n\r\n.inputForm {\r\n    border: 1.5px solid #ecedec;\r\n    border-radius: 10px;\r\n    height: 50px;\r\n    display: flex;\r\n    align-items: center;\r\n    padding-left: 10px;\r\n    transition: 0.2s ease-in-out;\r\n}\r\n\r\n.input {\r\n    margin-left: 10px;\r\n