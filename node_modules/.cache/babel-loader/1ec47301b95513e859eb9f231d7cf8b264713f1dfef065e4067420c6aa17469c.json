-form-input v-model=\"model\" required></b-form-input>\r\n        </b-form-group>\r\n\r\n        <b-form-group label=\"Size\" label-for=\"size\">\r\n          <b-form-input v-model=\"size\" type=\"number\" required></b-form-input>\r\n        </b-form-group>\r\n\r\n        <b-form-group label=\"Color\" label-for=\"color\">\r\n          <b-form-input v-model=\"color\" required></b-form-input>\r\n        </b-form-group>\r\n\r\n        <b-form-group label=\"Price\" label-for=\"price\">\r\n          <b-form-input v-model=\"price\" type=\"number\" required></b-form-input>\r\n        </b-form-group>\r\n\r\n        <b-form-group label=\"Image\" label-for=\"image\">\r\n          <b-form-file v-model=\"image\" accept=\"image/*\"></b-form-file>\r\n        </b-form-group>\r\n\r\n        <b-button type=\"submit\">Submit</b-button>\r\n      </b-form>\r\n    </b-modal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { BModal, BForm, BFormGroup, BFormInput, BFormFile, BButton } from 'bootstrap-vue';\r\nimport 'bootstrap/dist/css/bootstrap.css';\r\nimport 'bootstrap-vue/dist/bootstrap-vue.css';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      modalVisible: false,\r\n      brand: '',\r\n      model: '',\r\n      size: null,\r\n      color: '',\r\n      price: null,\r\n      image: null,\r\n    };\r\n  },\r\n  methods: {\r\n    openModal() {\r\n      this.modalVisible = true;\r\n    },\r\n    createShoe() {\r\n      const formData = new FormData();\r\n      formData.append('brand', this.brand);\r\n      formData.append('model', this.model);\r\n      formData.append('size', this.size);\r\n      formData.append('color', this.color);\r\n      formData.append('price', this.price);\r\n      formData.append('image', this.image);\r\n\r\n      axios.post('http://localhost:8080/create', formData)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          // Handle success\r\n        })\r\n        .catch(error => {\r\n          console.error('Error:', error);\r\n          // Handle error\r\n        })\r\n        .finally(() => {\r\n          this.modalVisible = false;\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAqCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,MAAM,EAAEC,KAAK,EAAEC,UAAU,EAAEC,UAAU,EAAEC,SAAS,EAAEC,OAAM,QAAS,eAAe;AACzF,OAAO,kCAAkC;AACzC,OAAO,sCAAsC;AAE7C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,KAAK;MACnBC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE,IAAI;MACVC,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,IAAI;MACXC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACV,IAAI,CAACR,YAAW,GAAI,IAAI;IAC1B,CAAC;IACDS,UAAUA,CAAA,EAAG;MACX,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/BD,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAACX,KAAK,CAAC;MACpCS,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAACV,KAAK,CAAC;MACpCQ,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACT,IAAI,CAAC;MAClCO,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAACR,KAAK,CAAC;MACpCM,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAACP,KAAK,CAAC;MACpCK,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAACN,KAAK,CAAC;MAEpCd,KAAK,CAACqB,IAAI,CAAC,8BAA8B,EAAEH,QAAQ,EAChDI,IAAI,CAACC,QAAO,IAAK;QAChBC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAChB,IAAI,CAAC;QAC1B;MACF,CAAC,EACAmB,KAAK,CAACC,KAAI,IAAK;QACdH,OAAO,CAACG,KAAK,CAAC,QAAQ,EAAEA,KAAK,CAAC;QAC9B;MACF,CAAC,EACAC,OAAO,CAAC,MAAM;QACb,IAAI,CAACpB,YAAW,GAAI,KAAK;MAC3B,CAAC,CAAC;IACN;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}